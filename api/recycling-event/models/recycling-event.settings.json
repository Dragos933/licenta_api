      "type": "integer",
      "required": true
    },
    "noBags": {
      "type": "integer",
      "required": true
    },
    "locationDeposit": {
      "type": "string",
      "required": true,
      "maxLength": 200
    },
    "description": {
      "type": "richtext",
      "required": true,
      "maxLength": 800
    },
    "requirements": {
      "type": "richtext",
      "maxLength": 800
    }
  }
}
